{
    "name": "cluster",
    "version": "1.0.0",
    "description": "Cluster management commands for Redis Shell.",
    "namespace": "/cluster",
    "commands": [
        {
            "name": "deploy",
            "description": "Deploy a new Redis cluster.",
            "usage": "/cluster deploy",
            "legacy_command": "deploycluster",
            "options": []
        },

        {
            "name": "info",
            "description": "Get information about the cluster.",
            "usage": "/cluster info",
            "legacy_command": "clusterinfo",
            "options": []
        },
        {
            "name": "remove",
            "description": "Remove the cluster and clean up.",
            "usage": "/cluster remove",
            "legacy_command": "removecluster",
            "options": []
        },
        {
            "name": "stop",
            "description": "Stop the cluster without cleaning up data.",
            "usage": "/cluster stop",
            "legacy_command": "stopcluster",
            "options": []
        },
        {
            "name": "start",
            "description": "Start the cluster without losing data.",
            "usage": "/cluster start",
            "legacy_command": "startcluster",
            "options": []
        }
    ],
    "completions": {},
    "author": "Redis Shell Team",
    "dependencies": {
        "redis": ">=5.0.0"
    }
}
